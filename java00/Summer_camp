public class Summer_camp
{
String name;
int grade;
int age;
public static void main(String[] args)
{
Summer_camp student1 = new Summer_camp();
	Summer_camp student2 = new Summer_camp();
	// In a Summer_camp there are so many students and we cant create 
	//an seperate object for each of them//
student1.name = "Karthick";
student1.grade = 1;
student1.age = 25;
student2.name = "Kavya";
student2.grade = 1;
student2.age = 20;
System.out.println(student1.name);
System.out.println(student1.grade);
System.out.println(student1.age);
System.out.println(student1.name);
System.out.println(student1.grade);
System.out.println(student1.age);
}
}